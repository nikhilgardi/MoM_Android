<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/login_REL_content"
    android:layout_width="fill_parent" android:layout_height="fill_parent" 
    android:padding="10dp" android:background="@drawable/appsbg">
	<RelativeLayout android:id="@+id/top_bar"
	    android:layout_width ="fill_parent" android:layout_height ="wrap_content"
	    android:layout_marginBottom="20dp" android:layout_marginTop="10dp"
	    android:orientation="vertical" android:gravity="top">
	    
		<TextView android:id="@+id/topbar_LBL_heading"
		    android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:typeface="normal" 
			android:gravity="center" android:paddingLeft="10dp" android:paddingRight="10dp"
		    android:text="@string/lbl_TransactionStatus" android:textSize="25sp" android:textColor="#464646" android:textStyle="bold"
		   
		    android:layout_alignParentTop="true" android:layout_centerInParent="true"/>
	</RelativeLayout>
    <RelativeLayout 
        android:layout_width="400dp" android:layout_height="fill_parent"
        android:layout_centerHorizontal="true" android:layout_below="@+id/top_bar"> 
        <RelativeLayout
		    android:layout_width ="fill_parent" android:layout_height = "wrap_content">
        <ScrollView android:id="@+id/lsttrxstatus_SCL_content"
            android:layout_width="fill_parent" android:layout_height="fill_parent">
        	<LinearLayout
			    android:layout_width ="fill_parent" android:layout_height = "wrap_content"
			    android:orientation="vertical">       	    
        	  	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent"
               	    android:layout_marginBottom="3dp">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblTxStatus"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	            	android:typeface="sans" 
	            	android:visibility="gone" 
	                android:text="@string/lbl_Status"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_TxStatus"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:typeface="sans"
		            android:textSize="22dp"
		            android:visibility="gone"
		            android:text=""/>
		        </LinearLayout>
        	    
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblTxDateTime"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	            	android:typeface="sans" 
	                android:text="@string/lbl_TxDate"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_TxDateTime"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:typeface="sans"
		            android:textSize="22dp"
		            android:text=""/>
		        </LinearLayout>
		        
		        <LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblCardholderName"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	                android:text="@string/lbl_Cardholder"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_CardholderName"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:typeface="sans"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:text=""/>
		        </LinearLayout>
      	    	
      	    	
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView 
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	                android:text="@string/lbl_AmountRs."/>
                		
		        <TextView android:id="@+id/lsttrxstatus_LBL_AmtRs"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:text=""/>
		        </LinearLayout>
      	    	
      	    	
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblCreditCardNum"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	                android:text="@string/lbl_Last4digits"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_CreditCardNum"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:text=""/>
		        </LinearLayout>
      	    			 		
       	    				    			    
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblTypeofTx"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	                android:text="@string/lbl_Type"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_TypeofTx"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:text=""/>
		        </LinearLayout>

       	    		    	
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblAuthNo"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	            	android:visibility="gone"
	                android:text="@string/lbl_AuthCode"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_AuthNo"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:visibility="gone"
		            android:text=""/>
		        </LinearLayout>
      	    		    	
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblRRNo"
               	    android:typeface="sans" 
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	            	android:visibility="gone"
	                android:text="@string/lbl_RRNo"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_RRNo"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:visibility="gone"
		            android:text=""/>
		        </LinearLayout>
     
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblStanID"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	                android:text="@string/lbl_StanId"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_StanID"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:text=""/>
		        </LinearLayout>
      	    		    	
      	    		    		   
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblVoucher"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	            	android:visibility="gone"
	                android:text="@string/lbl_Voucher"/>
		        <TextView android:id="@+id/lsttrxstatus_LBL_Voucher"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:visibility="gone"
		            android:text=""/>
		        </LinearLayout>
      	    			    			 		
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblTerminalMessage"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	            	android:visibility="gone"
	                android:text="@string/lbl_TxMessage"/>
		        <TextView android:id="@+id/lsttrxstatus_TXT_TerminalMessage"
		            android:layout_width="wrap_content" android:layout_height="wrap_content"
		            android:textColor="#000000"
		            android:textSize="22dp"
		            android:visibility="gone"
		            android:text=""/>
		        </LinearLayout>
      	    				    
      	    	<LinearLayout android:layout_width ="fill_parent" android:layout_height = "fill_parent" android:orientation="vertical"
      	    	    android:layout_marginBottom="10dp">
               	<TextView android:id="@+id/lsttrxstatus_LBL_lblNotes"
	            	android:textSize="17dp" android:textColor="#464646"
	            	android:typeface="sans" 
	            	android:layout_width="100dp" android:layout_height="wrap_content"
	            	android:visibility="gone"
	                android:text="@string/lbl_Notes"/>
		        <EditText android:id="@+id/lsttrxstatus_TXT_Notes"
				    android:layout_width="285dip" android:layout_height="120dip"
					android:layout_below="@+id/lsttrxstatus_LBL_lblNotes"
					android:background="@drawable/whiterectborderblack"
					android:visibility="gone"
				    android:maxLines="5" android:editable="false">		   
				</EditText>
		        
		        </LinearLayout>
    		</LinearLayout>
    	</ScrollView>
    	</RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
